{
    "name": "pinify",
    "version": "1.0.6",
    "description": "Simple and secure memos & notes, now with other people! (friends not included)",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "electron-toolkit": "electron ./node_modules/electron-toolkit",
        "postinstall": "electron-builder install-app-deps",
        "build": "build --win",
        "ship": "build --win -p always"
    },
    "keywords": [
        "electron",
        "pinify",
        "memo",
        "secure",
        "encryption"
    ],
    "repository": "https://github.com/andithemudkip/pinify",
    "author": "AndiTheMudkip",
    "license": "CC0-1.0",
    "devDependencies": {
        "electron": "^2.0.0",
        "electron-builder": "^20.15.1",
        "electron-rebuild": "^1.7.3",
        "electron-toolkit": "^1.0.24"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "date-and-time": "^0.6.2",
        "electron-context-menu": "^0.9.1",
        "electron-titlebar": "0.0.3",
        "electron-updater": "^2.21.10",
        "fb": "^2.0.0",
        "graceful-fs": "^4.1.11",
        "jquery": "^3.3.1",
        "metafetch": "^1.8.1",
        "node-notifier": "^5.2.1",
        "open": "0.0.5",
        "passport-discord": "^0.1.3",
        "request": "^2.86.0",
        "socket.io-client": "^2.1.1",
        "youtube-info": "^1.2.0"
    },
    "build": {
        "appId": "com.electron.Pinify",
        "copyright": "Copyright Â© 2018 AndiTheMudkip",
        "productName": "Pinify",
        "electronVersion": "2.0.0",
        "releaseInfo": {
            "releaseNotes": "Bug fixes, webshot previews, group images\nReworked Facebook login"
        },
        "asar": true,
        "directories": {
            "app": "C:\\Users\\picas\\Desktop\\Pinify",
            "output": "dist"
        },
        "win": {
            "target": [
                {
                    "target": "nsis",
                    "arch": [
                        "x64",
                        "ia32"
                    ]
                }
            ]
        },
        "protocols": [{
          "name": "pinify",
          "schemes": ["pinify"]
        }]
    }
}
